<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/feedImageView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/asda"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"></ImageView>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="160dp">

            <ImageButton
                android:id="@+id/feedLikeButton"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@null"
                android:src="@drawable/ic_baseline_favorite_border_24"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="160dp"></ImageButton>

            <TextView
                android:id="@+id/feedLikeCount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="1968"
                android:textSize="12dp"></TextView>

        </LinearLayout>

        <ImageButton
            android:id="@+id/feedShareButton"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@null"
            android:src="@drawable/ic_baseline_share_24"
            tools:layout_editor_absoluteX="345dp"
            tools:layout_editor_absoluteY="160dp"></ImageButton>



    </androidx.constraintlayout.widget.ConstraintLayout>



    <TextView
        android:id="@+id/feedTileTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FGHFHGFHGF"
        android:textSize="25dp"
        android:textStyle="bold"
        android:layout_marginLeft="25dp"></TextView>

    <TextView
        android:id="@+id/feedDescriptionTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Description"
        android:textSize="15dp"
        android:layout_marginLeft="25dp"></TextView>

    <TextView
        android:id="@+id/feedTypePrivateTextView"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        ></TextView>


    <TextView
        android:id="@+id/feedDocumentId"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        ></TextView>



</androidx.appcompat.widget.LinearLayoutCompat>